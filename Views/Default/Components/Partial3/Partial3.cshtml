@using Turn8.Models.Classes
@model List<Blog>

    <div class="padding">
        <h4>Admin's Top 10 Tracks</h4>
        <ul class="list list-pad">
            @{int i = 0;}
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++;} @i</span><a href="/blog/blogdetail/@x.Id">@x.Title</a></li>
        }
        </ul>
        <a href="/blog/index" class="link1">See all tracks</a>
    </div>
